{
  "manifest_version": 3,
  "name": "prompster",
  "version": "1.0.0",
  "description": "Slash Commands for ChatGPT",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/chat/*"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["static/prompts.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
