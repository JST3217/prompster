{
  "manifest_version": 2,
  "name": "prompster",
  "version": "1.0",
  "description": "Quick prompt injection in your Browser",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["bundle.js"],
      "css": ["bundle.css"]
    }
  ],
  "web_accessible_resources": ["..static/prompts.json"]
}
